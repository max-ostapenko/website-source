{
  "hosting": [
    {
      "site": "max-ostapenko",
      "public": "_site",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "predeploy": [
        "make render"
      ],
      "redirects": [
      ],
      "rewrites": [],
      "headers": [
        {
          "source": "site_libs/**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=2628000, must-revalidate"
            }
          ]
        },
        {
          "source": "static/**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=2628000, must-revalidate"
            }
          ]
        },
        {
          "source": "**/*.@(jpg|png|svg)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=2628000, must-revalidate"
            }
          ]
        }
      ]
    }
  ]
}
